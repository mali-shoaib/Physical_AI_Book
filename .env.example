# Environment Variables Template
# Copy this file to .env and fill in your actual values

# RAG Chatbot API Configuration
RAG_API_URL=http://localhost:8000

# OpenAI API (for embeddings and LLM)
OPENAI_API_KEY=sk-proj-your-openai-api-key-here


# Qdrant Vector Database
QDRANT_URL=https://your-qdrant-instance.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key-here

# Neon Postgres (for conversation history)
DATABASE_URL=postgres://user:password@host/database

# GitHub Pages Deployment (optional, for CI/CD)
GH_TOKEN=your-github-token
GitHub_link= "https://github.com/mali-shoaib/Physical_AI_Book.git"
Deployment= "https://physical-ai-robotics-textbook-xi.vercel.app/sitemap.xml"


# Build Configuration
NODE_ENV=production
