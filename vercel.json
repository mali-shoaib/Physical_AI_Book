{
  "version": 2,
  "name": "physical-ai-robotics-textbook",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/books/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    }
  ],
  "outputDirectory": "build"
}
